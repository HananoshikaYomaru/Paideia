{
  "name": "paideia",
  "private": true,
  "version": "0.0.1",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "bun scripts/build.ts",
    "docker:build": "docker build -t paideia:$(node -p \"require('./package.json').version\") .",
    "docker:run": "docker run -p 3000:3000 paideia:$(node -p \"require('./package.json').version\")",
    "docker:compose": "VERSION=$(node -p \"require('./package.json').version\") docker-compose up",
    "dev": "bun --watch ./server/index.ts",
    "typecheck": "react-router typegen && tsc --noEmit",
    "format": "prettier --ignore-path .gitignore --ignore-unknown --cache --cache-location node_modules/.cache/prettiercache --write .",
    "lint": "eslint --ignore-path .gitignore --no-error-on-unmatched-pattern --cache --cache-location node_modules/.cache/eslint --fix .",
    "start": "NODE_ENV=production bun ./server/index.ts"
  },
  "dependencies": {
    "@elysiajs/static": "^1.4.0",
    "@react-router/fs-routes": "^7.6.2",
    "elysia": "^1.3.5",
    "elysia-connect-middleware": "^0.0.6",
    "isbot": "^5.1.28",
    "node-cache": "^5.1.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.2"
  },
  "devDependencies": {
    "@react-router/dev": "^7.6.2",
    "@remix-run/eslint-config": "^2.16.8",
    "@types/bun": "^1.2.22",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9.29.0",
    "make-vfs": "^1.0.16",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "patchedDependencies": {
    "@react-router/dev@7.6.2": "patches/@react-router%2Fdev@7.6.2.patch"
  }
}
