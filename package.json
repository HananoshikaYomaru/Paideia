{
  "name": "paideia",
  "private": true,
  "version": "0.0.1",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "bun scripts/build.ts",
    "start": "./dist/paideia",
    "dev": "concurrently \"bun run docker:compose\" \"bun --watch ./server/index.ts\"",
    "docker:build": "docker build -t paideia:$(node -p \"require('./package.json').version\") .",
    "docker:run": "docker run -p 3000:3000 paideia:$(node -p \"require('./package.json').version\")",
    "docker:compose": "VERSION=$(node -p \"require('./package.json').version\") docker-compose up",
    "typecheck": "react-router typegen && tsc --noEmit",
    "format": "prettier --ignore-path .gitignore --ignore-unknown --cache --cache-location node_modules/.cache/prettiercache --write ."
  },
  "dependencies": {
    "@elysiajs/static": "^1.4.0",
    "@react-router/fs-routes": "^7.9.1",
    "elysia": "^1.4.7",
    "elysia-connect-middleware": "^0.0.6",
    "isbot": "^5.1.31",
    "node-cache": "^5.1.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.9.1"
  },
  "devDependencies": {
    "@react-router/dev": "^7.9.1",
    "@types/bun": "^1.2.22",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "concurrently": "^9.2.1",
    "make-vfs": "^1.0.16",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vite": "^6.3.6",
    "vite-plugin-devtools-json": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "patchedDependencies": {
    "@react-router/dev@7.6.2": "patches/@react-router%2Fdev@7.6.2.patch"
  }
}
