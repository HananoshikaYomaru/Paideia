{
  "name": "paideia",
  "private": true,
  "version": "0.0.1",
  "sideEffects": false,
  "type": "module",
  "author": "Paideia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hananoshikayomaru/paideia"
  },
  "homepage": "https://github.com/hananoshikayomaru/paideia",
  "scripts": {
    "build": "bun scripts/build.ts",
    "start": "./dist/paideia",
    "dev:local": "bun --watch ./server/index.ts",
    "dev": "VERSION=$(node -p \"require('./package.json').version\") docker-compose up",
    "docker:build": "docker build -t paideia:$(node -p \"require('./package.json').version\") .",
    "docker:run": "docker run -p 3000:3000 paideia:$(node -p \"require('./package.json').version\")",
    "typecheck": "react-router typegen && tsc --noEmit",
    "format": "prettier --ignore-path .gitignore --ignore-unknown --cache --cache-location node_modules/.cache/prettiercache --write ."
  },
  "dependencies": {
    "@bknd/postgres": "^0.2.0",
    "@elysiajs/static": "^1.4.0",
    "@neondatabase/serverless": "^1.0.1",
    "@react-router/fs-routes": "^7.9.1",
    "bknd": "^0.17.2",
    "elysia": "^1.4.7",
    "elysia-connect-middleware": "^0.0.6",
    "isbot": "^5.1.31",
    "kysely-neon": "^2.0.1",
    "node-cache": "^5.1.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.9.1"
  },
  "devDependencies": {
    "@react-router/dev": "^7.9.1",
    "@types/bun": "^1.2.22",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "make-vfs": "^1.0.16",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vite": "^6.3.6",
    "vite-plugin-devtools-json": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "patchedDependencies": {
    "@react-router/dev@7.6.2": "patches/@react-router%2Fdev@7.6.2.patch"
  }
}
