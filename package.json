{
  "name": "paideia",
  "private": true,
  "version": "0.0.1",
  "sideEffects": false,
  "type": "module",
  "author": "Paideia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hananoshikayomaru/paideia"
  },
  "homepage": "https://github.com/hananoshikayomaru/paideia",
  "scripts": {
    "build": "bun scripts/build.ts",
    "start": "./dist/paideia",
    "dev": "VERSION=$(node -p \"require('./package.json').version\") kill -9 $(lsof -t -i:3000) && docker-compose up -d && rm -rf node_modules/.vite node_modules/.cache node_modules/.vite-temp  && bun --watch ./server/index.ts",
    "docker:build": "docker build -t paideia:$(node -p \"require('./package.json').version\") .",
    "docker:run": "docker run -p 3000:3000 paideia:$(node -p \"require('./package.json').version\")",
    "typecheck": "react-router typegen && tsc --noEmit"
  },
  "dependencies": {
    "@elysiajs/static": "^1.4.0",
    "@mantine/core": "^8.3.2",
    "@mantine/form": "^8.3.2",
    "@mantine/hooks": "^8.3.2",
    "@payloadcms/db-postgres": "^3.56.0",
    "@react-router/fs-routes": "^7.9.1",
    "@scalar/api-reference-react": "^0.7.51",
    "elysia": "^1.4.7",
    "elysia-connect-middleware": "^0.0.6",
    "isbot": "^5.1.31",
    "mime": "^4.1.0",
    "node-cache": "^5.1.2",
    "payload": "^3.56.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.9.1",
    "remix-utils": "^9.0.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.9.1",
    "@types/bun": "^1.2.22",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "^5.9.2",
    "vite": "^6.3.6",
    "vite-plugin-devtools-json": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "patchedDependencies": {
    "@react-router/dev@7.6.2": "patches/@react-router%2Fdev@7.6.2.patch"
  }
}
